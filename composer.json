{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-gd": "*",
        "ext-zip": "*",
        "wikimedia/composer-merge-plugin": "^1.4",
        "funkjedi/composer-include-files": "^1.0",
        "mcaskill/composer-exclude-files": "^1.1",
        "mmucklo/krumo": "^0.6.0",
        "nwidart/laravel-modules": "^5.0",
        "johnpbloch/wordpress": "^5.2",
        "nunomaduro/collision": "^3.0",
        "encore/laravel-admin": "^1.7",
        "intervention/image": "^2.4",
        "spatie/eloquent-sortable": "^3.6",
        "ext-pdo": "*",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "almasaeed2010/adminlte": "~2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "doctrine/dbal": "^2.9",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "vendor-dir": "core/vendor/",
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "extra": {
        "include_files": [
            "library/helpers.php",
            "library/laravel-helpers.php",
            "library/admin-helpers.php"
        ],
        "merge-plugin": {
            "include": [
                "module/*/composer.json",
                "public/mall/*/composer.json"
            ],
            "require": [
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": false,
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        },
        "wordpress-install-dir": {
            "johnpbloch/wordpress-core": "public/app"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "exclude-from-files": [
            "laravel/framework/src/Illuminate/Foundation/helpers.php",
            "encore/laravel-admin/src/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "core/database/seeds",
            "core/database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "core/tests/"
        }
    },
    "repositories": {
        "local": {
            "type": "path",
            "url": "library/vendor/*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "php artisan ide-helper:meta -q",
            "php artisan ide-helper:models --write -q",
            "php artisan ide-helper:generate -q"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('core/.env') || copy('core/.env.example', 'core/.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
